<section class='container '>
	<h1> Search Date for Solar Activity </h1>
	<p> Boiler Plate for Science API Call </p>
  <center>
	<%= form_tag get_path, method: "get", html: { class: "form" } do %>
 	<div class="form-group col-sm-6 col-md-offset-3">
   <%= text_field_tag(:event_starttime, "", placeholder: " Start Date:  YYYY-MM-DD ", class: "form-control") %>
 	</div>
 	<div class="form-group col-sm-6 col-md-offset-3">
   	<%= text_field_tag(:event_endtime, "", placeholder: " End Date:  YYYY-MM-DD", class: "form-control") %>
 	</div>
</center>
 	<center><div class="form-group col-sm-12">
 	<%=submit_tag "Search", class: "btn btn-info", style:'width: 50%'%>
	<% end %>
</section>
